// RUN: %mapsci --layer2 --stop-after=layer2 < %s | filecheck %s

let a;
let b;
let c;
let d;

a +b          // CHECK: a + b
a -b          // CHECK: a - b
a- b          // CHECK: a - b
-(a + b)      // CHECK: - ( a + b )
(-a + -b) + c // CHECK: ( (-a) + (-b) ) + c
a - - b       // CHECK: a - (-b)
a* - b        // CHECK: a * (-b)
-a*b          // CHECK: (-a) * b
-a+ -b        // CHECK: (-a) + (-b)
a *b+c        // CHECK: ( a * b ) + c
a *b+c * d    // CHECK: ( ( a * b ) + c ) * d  
a (* b) + c   // CHECK: ( a * b ) + c          
a * b+c* d    // CHECK: a * ( b + ( c * d ) )  
a * b+c+ d    // CHECK: a * ( b + ( c + d ) )   
a * -b+c+ d   // CHECK: a * ( -b + ( c + d ) )  
