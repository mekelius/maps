rust-style:

fn name: type param -> type param -> return_type {
    return value
}


haskell-style:

name :: type -> type -> m type
name param param param = do
    expr
    expr
    return expr


c-style:

type name(type, type) {
    return value
}


our style:

let name = Type param -> Type param -> Type return:
    return = param + param